<table border>
<caption><h4>Listado de productos</h4></caption>
  <tr>    
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Precio unitario</th>
    <th>Existencias</th>
  </tr>

<% @productos.each do |producto| %>
  <tr>
    <td><%=h producto.Nombre %></td>
    <td><%=h producto.Descripcion %></td>
    <td><center><%=h producto.Precio_Unitario %></center></td>
    <td><center><%=h producto.Existencias %></center></td>
  </tr>
<% end %>
</table>
</br>
